{
  "redirects": [
    {
      "source": "/",
      "destination": "https://airdropalpha.vercel.app/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        }
      ]
    }
  ]
}